<div class="container-fluid p-1">
  <div class="row mx-auto">
    <div class="col-md-12">
      <div class="row">

        <div class="col-md-4" >
          <img alt="" src="aaa.png" width="770px">
        </div>
      </div>
    </div>

 <div class="row mx-auto " style="margin-bottom: 10px; margin:0%; padding:0%;">
    <table class="table table-borderless " style="font-size: 11px; border-color: blue; border: blue 1px solid; width: 624px; margin:0%; height: 25px;">
      <tbody>
        {{#each cliente}}
        <tr style="height: 10px;">
          <td colspan="2" style="border-bottom: blue 1px solid; padding: 0%;"><strong style="color: blue;">Cliente: </strong>{{Nombre}}</td>
        </tr>
        <tr style="height: 10px;">
          <td colspan="2" style="border-bottom: blue 1px solid; padding: 0%;"><strong style="color: blue;">Domicilio: </strong>{{DirCalle}} #{{DirNum}}</td>
        </tr>
        <tr style="height: 10px;">
          <td colspan="2" style="border-bottom: blue 1px solid; padding: 0%;"><strong style="color: blue;">Colonia: </strong>{{DirColonia}}</td>
        </tr>
        <tr style="height: 10px;">
          <td style="border-bottom: blue 1px solid; padding: 0%;"><strong style="color: blue;">Tel: </strong>{{Telefono}}</td>
          <td style="border-bottom: blue 1px solid; padding: 0%;"><strong style="color: blue;">Ciudad: </strong>{{Municipio}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>

    
    <table class="table table-borderless" style="border-color: blue; border: blue 1px solid; width: 156px; height: 73px; -webkit-print-color-adjust: exact; margin:0%; padding:0%;">
      <thead>
        <tr style="height: 20px;">
          <th colspan="3" style="height: 20px; background-color: blue; color:white; padding:0%;"><center style="height: 35px; font-size: 14px;">POLIZA DE GARANTÍA</center></th>
        </tr>
      </thead>
      <tbody>
        <tr style="border-bottom: blue 1px solid; border-right: blue 1px solid; font-size: 11px;">
          {{#each orden}}
         <td style="padding: 0%; border-right: blue 1px solid;"><center>Dia</center></td><td style="padding: 0%; border-right: blue 1px solid;"><center>Mes</center></td><td style="padding: 0%; border-right: blue 1px solid;"><center>Año</center></td>
          {{/each}}
        </tr>
        <tr style="font-size: 11px;">
           <td style="padding: 0%; border-right: blue 1px solid; font-size: 11px;"><center>{{momentDay FechaEntrega}}</center></td><td style="font-size: 11px; padding: 0%; border-right: blue 1px solid;"><center>{{momentMot FechaEntrega}}</center></td><td style="padding: 0%; border-right: blue 1px solid;"><center>{{momentYea FechaEntrega}}</center></td>
        </tr>
       
      </tbody>
    </table>
     
 
 </div>
 <div class="row mx-auto " style="margin:0%; padding:0%;">
  <table class="table table-borderless" style="border-color: blue; border: blue 1px solid; width: 780px; margin:0%; padding:0%;">
      <thead style="text-align: center;">
        <tr style="background-color: blue; height: 15px; margin:0%; padding:0%;">
          <th colspan="2" style="-webkit-print-color-adjust: exact; color: rgb(255, 255, 255); height: 15px; margin:0%; padding:0%; font-size: 14px; height: 14px;">Descripcion del servicio</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          {{#each orden}}
          <td colspan="2" style="border-bottom: blue 1px solid; padding: 0%; height: 200px; font-size: 11px;">{{Descripcion}}</td>
          {{/each}}
        </tr>
        <tr style="border-bottom: blue 1px solid; margin:0%; padding:0%; font-size: 13px; height: 15px;">
          <td style="-webkit-print-color-adjust: exact; background-color: blue; color:white; margin:0%; padding:0%; height: 15px;">Cantidad con letra</td><td style="-webkit-print-color-adjust: exact; margin:0%; height: 15px; padding:0%; background-color: blue; color:white;"><center>Total</center></td>
        </tr>
        <tr style="font-size:11px">
          <td style="border-right: blue 1px solid; margin:0%; padding:0%; height: 15px;">{{cantidad}}</td>
          {{#each orden}}
          <td style="margin:0%; padding:0%; height: 15px;"><center>${{CostoServicio}}</center></td>
          {{/each}}
        </tr>
      </tbody>
    </table>
 </div>
 <div class="row mx-auto " style="margin-bottom: 10px; margin:0%; padding:0%;">
    <table class="table table-borderless" style="width: 510px; margin:0%; height: 25px;">
      <tbody>
        <tr style="height: 25px;">
          <td style="padding: 0%;"><strong style="color: blue; font-size:14px">*En partes electricas no hay garantia </strong></td>
        </tr>
      </tbody>
    </table>


    <table class="table table-borderless" style="border-color: blue; border: blue 1px solid; width: 270px; -webkit-print-color-adjust: exact; margin:0%; padding:0%;">
      <thead>
        <tr>
          <th colspan="3" style="background-color: blue; color:white; padding:0%;"><center style="font-size:14px">Nombre del Tecnico</center></th>
        </tr>
      </thead>
      <tbody>
        <tr style="border-bottom: blue 1px solid; border-right: blue 1px solid;">
          {{#each tec}}
         <td style="padding: 0%; border-right: blue 1px solid;"><center style="font-size:11px">{{Nombre}}</center></td>
          {{/each}}
        </tr>
      </tbody>
    </table>
     
 
 </div>
 
</div>
</div>
</div>
</div>

